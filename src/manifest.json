{
    "manifest_version": 2,
    "author": "Magic Of Lolis",
    "background": {
        "page": "background.html"
    },
    "name": "Userscript+ for Tampermonkey",
    "description": "Show current site all UserJSï¼ŒThe easier way to install UserJs for Tampermonkey.",
    "version": "1.4.5",
    "homepage_url": "https://github.com/magicoflolis/Userscript-Plus",
    "icons": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "96": "images/icon_96.png",
        "128": "images/icon_128.png"
    },
    "browser_action": {
        "browser_style": false,
        "default_icon": {
            "16": "images/icon_16.png",
            "32": "images/icon_32.png",
            "96": "images/icon_96.png",
            "128": "images/icon_128.png"
        },
        "default_title": "Userscript+",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "webRequest",
        "<all_urls>"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "uscriptplus@mol.com",
            "strict_min_version": "55.0",
            "update_url": "https://raw.githubusercontent.com/magicoflolis/Userscript-Plus/master/dist/firefox/updates.json"
        }
    }
}